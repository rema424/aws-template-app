{
  "name": "my-app",
  "version": "1.0.0",
  "description": "## Set up project",
  "main": "index.js",
  "scripts": {
    "start:prod": "cross-env NODE_ENV=production node ./dist/server.js",
    "start:stg": "cross-env NODE_ENV=staging node ./dist/server.js",
    "start:test": "cross-env NODE_ENV=testing node ./dist/server.js",
    "start:dev": "cross-env NODE_ENV=development node ./dist/server.js",
    "dev": "node ./dist/server.js",
    "compile": "tsc",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint": "tslint --config tslint.json --project tsconfig.json --type-check 'src/**/*.ts'",
    "lint-format": "tslint --fix --project tsconfig.json 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf node_modules && npm i --production",
    "package": "rm -rf artifact.zip && zip -r artifact.zip dist node_modules Dockerfile .dockerignore package.json -x '**/*.map' '**/*.ts'  '**/*.md' '**/*.txt' '**/LICENSE'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.15",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.11",
    "@types/supertest": "^2.0.6",
    "aws-sdk": "^2.368.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-list-endpoints": "^4.0.0",
    "log4js": "^3.0.6"
  }
}
